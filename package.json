{
  "name": "construct-flow-ui",
  "version": "1.0.0",
  "description": "css framework for different goals",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "16.13.2",
    "npm": "8.1.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "./node_modules/.bin/eslint *.js",
    "prettier": "prettier --config .prettierrc --write *.js",
    "doc": "esdoc --out docs/src/ --readme ./README.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "precommit": "npm run prettier",
    "prepush": "call npm run lint && call npm run test",
    "commit": "git-cz",
    "storybook": "storybook dev -p 6006",
    "start": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConstructFlow/construct-flow-ui.git"
  },
  "keywords": [
    "css",
    "framework"
  ],
  "author": "Dmytro Holiak <dima.holiak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ConstructFlow/construct-flow-ui/issues"
  },
  "homepage": "https://github.com/ConstructFlow/construct-flow-ui#readme",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/web-components": "^7.0.7",
    "@storybook/web-components-webpack5": "^7.0.7",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "esdoc": "^1.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.3.0",
    "jest": "^29.0.0",
    "lerna": "^3.22.1",
    "lit": "^2.7.2",
    "prettier": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "precommit": "npm run lint",
      "prepush": "call npm run lint && call npm run test"
    }
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "jest-environment-node-single-context": "^29.0.0",
    "sass": "^1.57.1"
  }
}
